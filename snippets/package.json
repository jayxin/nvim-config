{
  "name": "snippets",
  "engines": {
    "vscode": "v0.0.1"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": "markdown",
        "path": "./snippets/markdown.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.json"
      },
      {
        "language": "go",
        "path": "./snippets/go.json"
      },
      {
        "language": "rust",
        "path": "./snippets/rust.json"
      },
      {
        "language": "tex",
        "path": "./snippets/tex/text.json"
      },
      {
        "language": "tex",
        "path": "./snippets/tex/math.json"
      },
      {
        "language": "tex",
        "path": "./snippets/tex/env.json"
      },
      {
        "language": "json",
        "path": "/./snippets/json.json"
      }
    ]
  }
}
